.header
    margin-bottom 20px
    padding 14px 14px 0 14px
    height 46px
    background-color #A60000
    color white
    text-align center
    text-transform uppercase
    letter-spacing 4px
    font-weight 400
    font-style normal
    font-size 18px

.title
    border-bottom 1px solid white

.background
    position fixed
    top 0
    left 0
    overflow auto
    min-height 100%
    width 100%
    height 100vh
    background-color #6fd2d2

.main
    margin 0 auto
    max-width 1280px
    width 90%
