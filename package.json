    "test:unit": "mocha test --compilers js:babel-core/register $(find test -path '*.spec.js')",
    "test:watch": "npm test -- --watch",
    "test:integration": "karma start --single-run",
    "build:clean": "rimraf ./dist",
    "build:prod": "npm run build:clean && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js -p ",
    "start": "npm run dev",
    "start:prod": "node src/server/index.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js --host $(hostname -I) --inline --hot --progress --colors",
    "babelize": "babel --watch=./src --out-dir=./babelbuild"
